{
  "name": "openai-embeddings-generator",
  "version": "1.0.0",
  "description": "Utility for generating and storing OpenAI embeddings",
  "type": "module",
  "main": "generate_embeddings.js",
  "scripts": {
    "example": "node generate_embeddings.js",
    "store": "node store_embeddings.js",
    "search": "node semantic_search.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:generate": "node --experimental-vm-modules node_modules/jest/bin/jest.js generate_embeddings.test.js",
